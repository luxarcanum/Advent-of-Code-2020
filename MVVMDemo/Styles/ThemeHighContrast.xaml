<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!-- Base Colours -->
    <SolidColorBrush x:Key="Background" Color="#000000" />

    <SolidColorBrush x:Key="ContainerPrimary" Color="navy" />
    <!-- navy -->
    <SolidColorBrush x:Key="ContainerSecondary" Color="#311B92" />
    <!--- 8% primary on Container Primary -->
    <SolidColorBrush x:Key="PrimaryLighter" Color="#00695C" />
    <SolidColorBrush x:Key="PrimaryColour" Color="#004D40" />
    <SolidColorBrush x:Key="PrimaryDarker" Color="#004D40" />
    <SolidColorBrush x:Key="SecondaryColour" Color="#3700B3" />
    <SolidColorBrush x:Key="ErrorColour" Color="#CF6679" />

    <SolidColorBrush x:Key="FontPrimary" Color="#FFFFFF" />
    <SolidColorBrush x:Key="FontDisabled" Color="Yellow" />
    <SolidColorBrush x:Key="Obnoxious" Color="Orange" />


    <SolidColorBrush x:Key="MenuBack" Color="Yellow" />
    <SolidColorBrush x:Key="MenuFont" Color="#000000" />

</ResourceDictionary>
